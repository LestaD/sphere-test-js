    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "nonstandard": "^0.10.3-toc",
    "rimraf": "^2.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "baobab": "^2.3.3",
    "baobab-react": "^1.0.1",
    "classnames": "^2.2.3",
